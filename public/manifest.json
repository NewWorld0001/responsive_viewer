{
  "manifest_version": 2,
  "name": "Responsive Viewer",
  "author": "Solaiman Kmail",
  "version": "1.0.11",
  "description": "Show multiple screens once, Responsive design tester",
  "icons": {
    "128": "16.png"
  },
  "browser_action": {
    "default_icon": "16.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "contentSettings"
  ]
}
